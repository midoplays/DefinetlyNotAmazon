{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport { toast } from 'bulma-toast';\nexport default {\n  name: 'SignUp',\n  data() {\n    return {\n      firstname: '',\n      lastname: '',\n      email: '',\n      password: '',\n      password2: '',\n      birthdate: '',\n      errors: []\n    };\n  },\n  methods: {\n    submitForm() {\n      this.error = [];\n      if (this.firstname === '') {\n        this.errors.push('the first name is missing');\n      }\n      if (this.lastname === '') {\n        this.errors.push('the last name is missing');\n      }\n      if (this.email === '') {\n        this.errors.push('the email is missing');\n      }\n      if (this.password === '') {\n        this.errors.push('password is too short');\n      }\n      if (this.password != this.password2) {\n        this.errors.push('passwords do not match');\n      }\n      if (this.birthdate === '') {\n        this.errors.push('birth date is missing');\n      }\n      if (!this.errors.length) {\n        const formData = {\n          firstname: this.firstname,\n          lastname: this.lastname,\n          email: this.email,\n          password: this.password,\n          birthdate: this.birthdate\n        };\n        axios.post(\"http\", formData).then(response => {\n          toast({\n            message: 'Account created, please log in!',\n            type: 'is-success',\n            dismissible: true,\n            pauseOnHover: true,\n            duration: 2000,\n            position: 'bottom-right'\n          });\n          this.$router.push('/log-in');\n        }).catch(error => {\n          if (error.response) {\n            for (const property in error.response.data) {\n              this.errors.push(`${property}: ${error.response.data[property]}`);\n            }\n            console.log(JSON.stringify(error.response.data));\n          } else if (error.message) {\n            this.errors.push('Something went wrong. Please try again');\n            console.log(JSON.stringify(error));\n          }\n        });\n      }\n    }\n  }\n};","map":{"version":3,"mappings":";AA4DA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,KAAI,QAAS,aAAY;AAElC,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,IAAI,GAAG;IACH,OAAO;MACHC,SAAS,EAAE,EAAE;MACbC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE,EAAE;MACbC,SAAS,EAAE,EAAE;MACbC,MAAM,EAAE;IACZ;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,UAAU,GAAG;MACT,IAAI,CAACC,KAAI,GAAI,EAAC;MAEd,IAAI,IAAI,CAACT,SAAQ,KAAM,EAAE,EAAE;QACvB,IAAI,CAACM,MAAM,CAACI,IAAI,CAAC,2BAA2B;MAChD;MACA,IAAI,IAAI,CAACT,QAAO,KAAM,EAAE,EAAE;QACtB,IAAI,CAACK,MAAM,CAACI,IAAI,CAAC,0BAA0B;MAC/C;MACA,IAAI,IAAI,CAACR,KAAI,KAAM,EAAE,EAAE;QACnB,IAAI,CAACI,MAAM,CAACI,IAAI,CAAC,sBAAsB;MAC3C;MACA,IAAI,IAAI,CAACP,QAAO,KAAM,EAAE,EAAE;QACtB,IAAI,CAACG,MAAM,CAACI,IAAI,CAAC,uBAAuB;MAC5C;MACA,IAAI,IAAI,CAACP,QAAO,IAAK,IAAI,CAACC,SAAS,EAAE;QACjC,IAAI,CAACE,MAAM,CAACI,IAAI,CAAC,wBAAwB;MAC7C;MACA,IAAI,IAAI,CAACL,SAAQ,KAAM,EAAE,EAAE;QACvB,IAAI,CAACC,MAAM,CAACI,IAAI,CAAC,uBAAuB;MAC5C;MAEA,IAAI,CAAC,IAAI,CAACJ,MAAM,CAACK,MAAM,EAAE;QACrB,MAAMC,QAAO,GAAI;UACdZ,SAAS,EAAE,IAAI,CAACA,SAAS;UACzBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBE,SAAS,EAAE,IAAI,CAACA;QACnB;QAEAT,KAAI,CACCiB,IAAI,CAAC,MAAM,EAAED,QAAQ,EACrBE,IAAI,CAACC,QAAO,IAAK;UACdlB,KAAK,CAAC;YACFmB,OAAO,EAAE,iCAAiC;YAC1CC,IAAI,EAAE,YAAY;YAClBC,WAAW,EAAE,IAAI;YACjBC,YAAY,EAAE,IAAI;YAClBC,QAAQ,EAAE,IAAI;YACdC,QAAQ,EAAE;UACd,CAAC;UACD,IAAI,CAACC,OAAO,CAACZ,IAAI,CAAC,SAAS;QAC/B,CAAC,EACAa,KAAK,CAACd,KAAI,IAAK;UACZ,IAAIA,KAAK,CAACM,QAAQ,EAAE;YAChB,KAAK,MAAMS,QAAO,IAAKf,KAAK,CAACM,QAAQ,CAAChB,IAAI,EAAE;cACxC,IAAI,CAACO,MAAM,CAACI,IAAI,CAAE,GAAEc,QAAS,KAAIf,KAAK,CAACM,QAAQ,CAAChB,IAAI,CAACyB,QAAQ,CAAE,EAAC;YACpE;YACAC,OAAO,CAACC,GAAG,CAACC,IAAI,CAACC,SAAS,CAACnB,KAAK,CAACM,QAAQ,CAAChB,IAAI,CAAC;UACnD,OAAO,IAAIU,KAAK,CAACO,OAAO,EAAE;YACtB,IAAI,CAACV,MAAM,CAACI,IAAI,CAAC,wCAAwC;YAEzDe,OAAO,CAACC,GAAG,CAACC,IAAI,CAACC,SAAS,CAACnB,KAAK,